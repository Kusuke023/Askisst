<%- include('./_header') %>

    <div class="kyatti text-center">
        <br>
        <h1>仕事依頼表示画面</h1>
    </div>

    <!-- コンテンツ -->
    <main class="">
        <% rquests.forEach((rquest) => { %>
            <div class="m-3 p-3 bg-light shadow-sm rounded">
                <p>
                    仕事内容詳細<br>
                </p>
                <hr>
                <a href="#" class="" style="color: black; text-decoration: none;">
                <div class="p">
                    <label>内容</label>
                    <p><%= rquest.job_des %></p>
                    <br>
                    <label>報酬金</label>
                    <p><%= rquest.reward %></p>
                    <br>
                    <label>交通費支給</label>
                    <p><%= rquest.carfare %></p>                  
                    <br>
                    <label>勤務時間</label>
                    <p><%= rquest.work_time %></p>
                    <img src="<%= rquest.job_img %>" style="width: 100%;height: auto;">            
                </div>
            </a>
                <hr>
                <div class="text-center">
                    <a href="/" id="btn" class="m-2 px-5 btn btn-primary" onclick="send()">応募する</a>
                </div>
            </div>
        <% }) %>

    </main>
    <!-- 広告-->
